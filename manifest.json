{
  "name": "Home Shortcut",
  "version": "1.0.3",
  "description": "Adds a keyboard shortcut for going to a site's home page.",
  "manifest_version": 3,
  "icons": { 
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "action": {},
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
